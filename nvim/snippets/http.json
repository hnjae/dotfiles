    "body": "-c \"${0:filename}\""
  },
  "sexample": {
    "prefix": "sexample",
    "body": [
      "@server = http://${1:localhost}:${2:3000}",
      "",
      "POST {{server\\}\\}comments",
      "content-type: application/json",
      "",
      "# NOTE: json comma 조심!",
