{
  "name": "my-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "markdown"
        ],
        "path": "./snippets/markdown.jsonc"
      },
      {
        "language": [
          "sh"
        ],
        "path": "./snippets/sh.jsonc"
      },
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global"
        ],
        "path": "./snippets/license.jsonc"
      },
      {
        "language": [
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global",
          "all"
        ],
        "path": "./snippets/global.jsonc"
      },
      {
        "language": [
          "javascript"
        ],
        "path": "./snippets/javascript.jsonc"
      },
      {
        "language": [
          "typescript"
        ],
        "path": "./snippets/typescript.jsonc"
      },
      {
        "language": [
          "typescriptreact"
        ],
        "path": "./snippets/typescriptreact.jsonc"
      },
      {
        "language": "python",
        "path": "./snippets/python.jsonc"
      },
      {
        "language": "lua",
        "path": "./snippets/lua.jsonc"
      },
      {
        "language": "asciidoc",
        "path": "./snippets/asciidoc.jsonc"
      },
      {
        "language": "http",
        "path": "./snippets/http.jsonc"
      },
      {
        "language": "nix",
        "path": "./snippets/nix.jsonc"
      },
      {
        "language": "jsonc",
        "path": "./snippets/jsonc.jsonc"
      }
    ]
  }
}
