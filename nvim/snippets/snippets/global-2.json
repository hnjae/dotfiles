{
  "__duration": {
    "prefix": "__duration",
    "description": "ISO 8601 duration format",
    "body": "P${1:nYnMnD}T${2:nHnMnS}"
  },
  "vi-modeline": {
    "prefix": "vim",
    "body": "${LINE_COMMENT} vi:ft=${0:filetype}:${1:tw=80}",
    "description": "Vi's modeline"
  },
  "NOTE": {
    "prefix": "note",
    "body": "${LINE_COMMENT} NOTE: $0 <${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}>",
    "description": "Write NOTE (@comment.note)"
  },
