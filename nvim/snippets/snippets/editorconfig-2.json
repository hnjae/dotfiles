{
    "boilerplate": {
        "prefix": "boilerplate",
        "description": "boilerplate",
        "body": [
            "root = true",
            "",
            "[*]",
            "charset = utf-8",
            "end_of_line = lf",
            "insert_final_newline = true",
            "trim_trailing_whitespace = true",
            "tab_width = 8",
            "indent_size = 8",
            "indent_style = tab",
            "",
            "[*.{ts,js\\}]",
            "max_line_length = 100",
            "indent_size = 2",
            "indent_style = space"
        ]
    }
}
