{
  "spostjson": {
    "prefix": "spostjson",
    "description": "\"post json\"",
    "body": [
      "POST http://${1:url}",
      "content-type: application/json",
      "{",
      "\t${0:json-body}",
      "\\}",
      ""
    ]
  },
  "spostjsonopt": {
    "prefix": "spostjsonopt",
    "description": "\"POST json body with options\"",
    "body": [
      "POST http://{{host\\}\\}",
      "content-type: application/json",
      "[Options]",
      "variable: host = \"${1:localhost:3000}\"",
      "{",
      "\t\"$0\"",
      "\t\"name\": \"{{newUuid\\}\\}\",",
      "\t\"order\": 0",
      "\\}",
      ""
    ]
  },
  "suuid": {
    "prefix": "suuid",
    "description": "\"newUuid\"",
    "body": [
      "{{newUuid\\}\\}",
      ""
    ]
  },
  "sint": {
    "prefix": "sint",
    "description": "\"random-int-number\"",
    "body": "{{random_int_number\\}\\}"
  }
}
