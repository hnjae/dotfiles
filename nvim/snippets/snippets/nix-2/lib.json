    "body": "types.str;"
  },
  "tnonemptystr": {
    "prefix": "tnonemptystr",
    "description": "nonEmptyStr",
    "body": ["types.nonEmptyStr", ""]
  },
  "newtype": {
    "prefix": "newtype",
    "description": "new-type",
    "body": [
      "nonEmptyStr = mkOptionType {",
      "\tname = \"nonEmptyStr\";",
      "\tdescription = \"non-empty string\";",
      "\tdescriptionClass = \"noun\";",
      "\tcheck = x: str.check x && builtins.match \"[ tn]*\" x == null;",
      "\tinherit (str) merge;",
      "\\};",
      ""
    ]
  }
