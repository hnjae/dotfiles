    "body": ["lib.mkOptionDefault ${0:value}", ""]
  },
  "mkDefault": {
    "prefix": "mkDefault",
    "description": "lib.mkOverride 1000",
    "body": ["lib.mkDefault ${0:value}", ""]
  },
