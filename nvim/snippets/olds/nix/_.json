{
  "sdefault.nix": {
    "prefix": "sdefault.nix",
    "description": "\"default.nix\"",
    "body": [
      "{pkgs ? import <nixpkgs> {\\}\\}:",
      "$0"
    ]
  },
  "b": {
    "prefix": "b",
    "description": "\"bracket\"",
    "body": "[$0];"
  },
  "p": {
    "prefix": "p",
    "description": "\"parentheses\"",
    "body": "($0);"
  },
  "if": {
    "prefix": "if",
    "description": "if-then-else",
    "body": "if (${1:true}) then ($2) else ($3)"
  },
  "im": {
    "prefix": "im",
    "description": "\"import other nix file\"",
    "body": "import ${0:path-to-other-nix}"
  },
  "funs": {
    "prefix": "funs",
    "description": "\"example of function that expects a set\"",
    "body": "{ x, y ? \"bar\", ... \\} @ args: x + y"
  },
  "fun": {
    "prefix": "fun",
    "body": [
      "{${1:...}\\}: {",
      "\t$0",
      "\\}"
    ]
  },
  "ims": {
    "prefix": "ims",
    "body": [
      "imports = [",
      "\t$0",
      "];"
    ]
  }
}
