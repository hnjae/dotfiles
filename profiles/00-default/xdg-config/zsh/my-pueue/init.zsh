# https://github.com/Nukesor/pueue

if (( ! $+commands[pueue] )); then
  return
fi

alias -- pu=pueue
alias -- puK='pueue kill'
alias -- puA='pueue add'
alias -- puCl='pueue clean'
alias -- puEd='pueue edit'
alias -- puEq='pueue enqueue'
alias -- puFo='pueue follow'
alias -- puGr='pueue group'
alias -- puH='pueue help'
alias -- puL='pueue log'
alias -- puPs='pueue pause'
alias -- puRm='pueue remove'
alias -- puRx='pueue restart'
alias -- puSd='pueue send'
alias -- puSh='pueue stash'
alias -- puSt='pueue status'
alias -- puSw='pueue switch'
alias -- puW='pueue wait'
alias -- puX='pueue start'
