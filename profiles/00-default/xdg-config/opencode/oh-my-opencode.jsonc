{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  // https://github.com/code-yeongyu/oh-my-opencode/blob/dev/docs/configurations.md
  "agents": {
    ////////////////
    // CORE AGENT //
    ////////////////

    // The default orchestrator. Plans, delegates, and executes complex tasks using specialized subagents with aggressive parallel execution. Todo-driven workflow with extended thinking (32k budget).
    // default: opus
    "Sisyphus": {
      "model": "opencode/gemini-3-flash"
      // "model": "opencode/glm-4.7"
      // 2026-01-23 Í∏∞Ï§Ä gpt-5.2-codex Îäî todo Í∞Ä ÏïàÎê®.
    },

    // Architecture decisions, code review, debugging. Read-only consultation - stellar logical reasoning and deep analysis. Inspired by AmpCode.
    "oracle": {
      "model": "opencode/gpt-5.2", // Use GPT for debugging
      "variant": "xhigh"
    },

    // Multi-repo analysis, documentation lookup, OSS implementation examples. Deep codebase understanding with evidence-based answers. Inspired by AmpCode.
    "librarian": {
      "model": "opencode/glm-4.7-free" // Cheap model for research
    },

    // Fast codebase exploration and contextual grep. Uses Gemini 3 Flash when Antigravity auth is configured, Haiku when Claude max20 is available, otherwise Grok. Inspired by Claude Code.
    "explore": {
      "model": "openrouter/deepseek/deepseek-v3.2" // cheap model to grep
    },

    // Visual content specialist. Analyzes PDFs, images, diagrams to extract information. Saves tokens by having another agent process media.
    "multimodal-looker": {
      "model": "opencode/gemini-3-flash"
    },

    ///////////////////
    // Planing Agent //
    //////////////////

    // üîÆ Prometheus (The Planner)
    // Model: anthropic/claude-opus-4-5
    // Role: Strategic planning, requirements interviews, work plan creation
    // Constraint: READ-ONLY. Can only create/modify markdown files within .sisyphus/ directory.
    // Characteristic: Never writes code directly, focuses solely on "how to do it".
    // Strategic planner with interview mode. Creates detailed work plans through iterative questioning.
    // default opus
    "Prometheus (Planner)": {
      "model": "opencode/gpt-5.2-codex",
      "variant": "xhigh"
      // "model": "opencode/gemini-3-pro",
      // "variant": "high"
    },

    // ü¶â Metis (The Plan Consultant)
    // Role: Pre-analysis and gap detection
    // Function: Identifies hidden user intent, prevents AI over-engineering, eliminates ambiguity.
    // Workflow: Metis consultation is mandatory before plan creation.

    // Plan consultant - pre-planning analysis. Identifies hidden intentions, ambiguities, and AI failure points.
    // default: sonnet
    "Metis (Plan Consultant)": {
      "model": "opencode/gemini-3-flash"
    },

    // ‚öñÔ∏è Momus (The Plan Reviewer)
    // Role: High-precision plan validation (High Accuracy Mode)
    // Function: Rejects and demands revisions until the plan is perfect.
    // Trigger: Activated when user requests "high accuracy".
    // Plan reviewer - validates plans against clarity, verifiability, and completeness standards.
    // default: sonnet
    "Momus (Plan Reviewer)": {
      "model": "opencode/gemini-3-flash"
    },

    // ‚ö° Atlas (The Plan Executor)
    // default: opus
    // Role: Execution and delegation
    // Characteristic: Doesn't do everything directly, actively delegates to specialized agents (Frontend, Librarian, etc.).
    "Atlas": {
      "model": "opencode/gpt-5.2",
      "variant": "xhigh"
      // "model": "opencode/gemini-3-pro",
      // "variant": "high"
    }
  },

  // https://github.com/code-yeongyu/oh-my-opencode/blob/dev/docs/category-skill-guide.md
  "categories": {
    // Deep logical reasoning, complex architecture decisions requiring extensive analysis
    // default: gpt-5.2-codex
    "ultrabrain": {
      "model": "opencode/gpt-5.2-codex",
      "variant": "xhigh"
    },
    // Trivial tasks - single file changes, typo fixes, simple modifications
    // default: haiku
    "quick": {
      "model": "openrouter/deepseek/deepseek-v3.2"
    },
    // Tasks that don't fit other categories, low effort required
    // default: sonnet
    "unspecified-low": {
      "model": "opencode/gemini-3-flash"
    },
    // Tasks that don't fit other categories, high effort required
    // default: opus
    "unspecified-high": {
      "model": "opencode/gpt-5.2",
      "variant": "xhigh"
    },
    // Documentation, prose, technical writing
    "writing": {
      "model": "opencode/claude-sonnet-4-5"
    }
  },
  "lsp": {
    "ty": {
      "command": ["ty", "server"],
      "extensions": [".py", ".pyi"],
      "priority": 10
    },
    "pyright": {
      "disabled": true
    },
    "basedpyright": {
      "disabled": true
    }
  },
  "git_master": {
    "commit_footer": true,
    "include_co_authored_by": true
  },
  "experimental": {
    "auto_resume": true
  }
}
