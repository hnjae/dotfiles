  "lagenAttrsp": {
    "prefix": "lagenAttrsp",
    "description": "Generate attrs from list",
    "body": [
      "(",
      "\tbuiltins.listToAttrs (",
      "\t\tmap (",
      "\t\t\t${2:element}: {",
