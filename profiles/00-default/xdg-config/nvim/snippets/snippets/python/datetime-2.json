{
  "UTC ISO-8601 datetime string": {
    "prefix": "striso8601",
    "description": "use this with strftime",
    "body": "%Y%m%dT%H%M%S%z"
  },
  "import datetime": {
    "prefix": ["sfimdt", "simdt"],
    "body": "from datetime import datetime\n$0"
  },
  "import timezone": {
    "prefix": ["sfimtz", "simtz"],
    "body": "from datetime import timezone\n$0"
  },
  "now without microsecond (UTC)": {
    "prefix": "snowm",
    "description": "UTC now without microsecond (datetime)",
    "body": "datetime.now(tz=UTC).replace(microsecond=0)"
  },
  "now without microseconds (localtime zone)": {
    "prefix": "snowlm",
    "description": "localtime zone now without microseconds (datetime)",
    "body": "datetime.now().replace(microsecond=0).astimezone()"
  },
  "now (localtime zone)": {
    "prefix": "snowl",
    "description": "localtime zone now (datetime)",
    "body": "datetime.now().astimezone()"
  },
  "now (UTC)": {
    "prefix": "snow",
    "description": "UTC now (datetime)",
    "body": "datetime.now(tz=UTC)"
  },
  "today (utc)": {
    "prefix": "stoday",
    "description": "UTC today (date)",
    "body": "datetime.now(tz=UTC).date()"
  },
  "timezone seoul": {
    "prefix": "stzseoul",
    "description": "",
    "body": "ZoneInfo(key=\"Asia/Seoul\")"
  }
}
