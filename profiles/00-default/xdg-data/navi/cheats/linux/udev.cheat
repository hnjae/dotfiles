% udev

# Walk device attributes (includes parents)
udevadm info --attribute-walk  <device>

# query all device info (P: Path; N: Name (KERNEL), E: Environment, S: Symbolic Link)
udevadm info --query=all --name="/dev/<device>"

# query all device properties (Environment)
udevadm info --query=property --name="/dev/<device>"

# reload & trigger
sudo udevadm control --reload-rules && sudo udevadm trigger

# Print and follow log
journalctl --output=short-full --boot=0  -f -u systemd-udevd
