% journalctl, log, systemd

# print last boot systemd log (-b: --boot[=ID])
journalctl -b -1

# print last boot systemd log (only kernel)(-k: --dmesg)
journalctl --b -1 -k

# print (-x: --catalog; -u: --unit=)
journalctl --user -xu <systemd-units>

# print log in realtime (-f: --follow)
journalctl -f --boot=0 --output=short-full --unit=<systemd-units>

# show log size
journalctl --log-size
